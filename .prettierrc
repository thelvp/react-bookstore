{
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "printWidth": 80,
  "htmlWhitespaceSensitivity": "ignore",
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "tailwindStyleSheet": "./src/styles/App.css",
  "tailwindConfig": "tailwind.config.ts",
  "tailwindFunctions": ["clsx"],
  "plugins": ["prettier-plugin-tailwindcss"]
}
